{
  "svg":"HighwayEnv.svg",
  "definitions":[
    { "name":"xoffset",
      "value" :"100.0",
      "comment":"x coordinate of EgoVehicle in visualization"
    },
    { "name":"yoffset",
      "value" :"75.0",
      "comment":"y coordinate of EgoVehicle in visualization"
    },
    { "name":"vehicle_height",
       "value" :"20.0"
    },
    { "name":"vehicle_length",
       "value" :"50.0"
    },
    { "name":"ego_xpos",
      "value":"xoffset - 10.0 * 2.0/2.0"
    },
    { "name":"ego_ypos",
      "value":"yoffset + 10.0 * (VehiclesY(EgoVehicle) - 5.0/2.0)"
    },
    { "name":"xpos",
      "value":"%v.(v:Vehicles|xoffset + 10.0 * (VehiclesX(v) - 2.0/2.0))"
    },
    { "name":"ypos",
      "value":"%v.(v:Vehicles|yoffset + 10.0 * (VehiclesY(EgoVehicle) + VehiclesY(v) - 5.0/2.0))"
    }
  ],
  "svg_objects":[
    {
      "svg_class":"rect",
      "id":"ego",
      "fill":"red",
      "visibility":"visible",
      "width":"50",
      "height":"20",
      "x":"100",
      "y":"90",
      "rx":"5",
      "title":"Ego vehicle"
    },
    {
      "for":{"from":2,"to":"card(Vehicles)"}, 
      "svg_class":"rect",
      "id":"car_%0",
      "fill":"blue",
      "visibility":"visible",
      "width":"50",
      "height":"20",
      "x":"100",
      "y":"90",
      "rx":"5",
      "title":"other vehicle"
    },
    {
      "for":{"from":1,"to":"card(Vehicles)"}, 
      "svg_class":"line",
      "id":"speed%0",
      "stroke":"white",
      "visibility":"visible",
      "stroke-width":"0.8",
      "marker-start": "url(#circlemarker)",
      "marker-end":"url(#arrowhead)"
    },
    {
      "svg_class":"text",
      "id":"infotext",
      "x":100,
      "y":180,
      "stroke":"black",
      "text": "",
      "comment": "used to display hover informations"
    }
  ],
  "items":[
    {
      "id": "ego",
      "x":"ego_xpos",
      "y":"ego_ypos",
      "title":"```Ego vehicle at x:${VehiclesX(EgoVehicle)}, y:${VehiclesY(EgoVehicle)}```"
    },
    {
      "for":{"from":2,"to":"card(Vehicles)"}, 
      "id": "car_%0",
      "x":"xpos(Vehicles%0)",
      "y":"ypos(Vehicles%0)",
      "title":"```Vehicle %0 at x:${VehiclesX(Vehicles%0)}, y:${VehiclesY(Vehicles%0)}```",
      "visibility":"IF Vehicles%0 : PresentVehicles THEN \"visible\" ELSE \"hidden\" END"
    },
    {
      "for":{"from":2,"to":"card(Vehicles)"}, 
      "id": "speed%0",
      "attr": "x1",
      "value":"xpos(Vehicles%0)+vehicle_length/2.0"
    },
    {
      "for":{"from":2,"to":"card(Vehicles)"}, 
      "id": "speed%0",
      "attr": "y1",
      "value":"ypos(Vehicles%0) + vehicle_height/2.0"
    },
    {
      "for":{"from":2,"to":"card(Vehicles)"}, 
      "id": "speed%0",
      "attr": "x2",
      "value":"xpos(Vehicles%0)+1.0*(VehiclesVx(Vehicles%0) + VehiclesVx(EgoVehicle))+vehicle_length/2.0"
    },
    {
      "for":{"from":2,"to":"card(Vehicles)"}, 
      "id": "speed%0",
      "attr": "y2",
      "value":"ypos(Vehicles%0)+1.0*(VehiclesVy(Vehicles%0) + VehiclesVy(EgoVehicle)) + vehicle_height/2.0"
    },
    {
      "id": "speed1",
      "attr": "x1",
      "value":"ego_xpos+vehicle_length/2.0"
    },
    {
      "id": "speed1",
      "attr": "y1",
      "value":"ego_ypos + vehicle_height/2.0"
    },
    {
      "id": "speed1",
      "attr": "x2",
      "value":"ego_xpos+1.0*VehiclesVx(EgoVehicle)+vehicle_length/2.0"
    },
    {
      "id": "speed1",
      "attr": "y2",
      "value":"ego_ypos+1.0*VehiclesVy(EgoVehicle) + vehicle_height/2.0"
    }
  ],
  "events": [
    {
	  "for": {"from":1, "to":"card(Vehicles)"},
      "id": "car_%0",
      "event": "",
      "hovers": [{ "attr":"opacity", "enter":"0.5", "leave":"1"},
                 { "id": "speed%0","attr":"visibility", "enter":"visible", "leave":"hidden"},
                 { "id": "infotext","attr":"text", "enter":"Vehicle%0", "leave":""}]
    },
    {
      "id": "ego",
      "event": "",
      "hovers": [{ "attr":"opacity", "enter":"0.5", "leave":"1"},
                 { "id": "speed1","attr":"visibility", "enter":"visible", "leave":"hidden"},
                 { "id": "speed1","attr":"stroke", "enter":"red", "leave":"white"},
                 { "id": "infotext","attr":"text", "enter":"EgoVehicle", "leave":""}]
    }
    ]
}
